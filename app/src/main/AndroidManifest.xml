<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="android.valkyrie.com.istay">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".admin.MainActivity" />
        <activity android:name=".admin.AddBoardingHouse" />

        <activity android:name=".admin.AddManager"
            android:parentActivityName=".admin.MainActivity"/>

        <activity android:name=".BoardingHouseInfo" />
        <activity android:name=".ManagerInfo" />
        <activity android:name=".admin.UpdateBoardingHouse" />
        <activity android:name=".admin.UpdateManager" />
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Signup"
            android:parentActivityName=".Login">
        </activity>
        <activity android:name=".HomeActivity" />
        <activity android:name=".AddReview"></activity>
    </application>

</manifest>